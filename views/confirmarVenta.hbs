<nav class="navegacion">
    <a href="javascript:history.back()"><i class="bi bi-box-arrow-left"></i></a>
    <div>
      <span>Ventas</span>
    </div>
    <div class="iconMenuHamburguesa">
      <i class="bi bi-menu-button-wide-fill"></i>
    </div>
  </nav>
   <div class="contMayorCli">
<div class="formCliente">
    <div class="divClienteBuscar">
        <h3 class="tituloForm">Confirmar Venta</h3>
      </div>
    <form action="/ventas/guardar" method="post" class="formCliente2">

      <p>Confirmar Datos De Venta</p>
      <br />

      <label class="title" for="nombre">Apellido y Nombre</label>
      <input type="text" class="inpForm" value="{{cliente.nombre}}" id="nombre" name="nombre" required readonly/>
      <br />
      <label class="title" for="dni">DNI</label>
      <input type="number" class="inpForm" value="{{cliente.dni}}" id="dni" name="dni" required readonly/>
      <br />
      <label class="title" for="direccion">Direccion</label>
      <input type="text" class="inpForm" value="{{cliente.localidadComercial}} / {{cliente.dirComercial}}" id="direccion" name="direccion" required readonly/>
      <br />
      <label class="title" for="dni">Celular</label>
      <input type="number" class="inpForm" value="{{cliente.celular}}" id="celular" name="celular" required readonly/>
      <br />
      <label class="title" for="venRuta">Vendedor</label>
      <input type="number" class="inpForm" value="{{cot.venRuta}}" id="venRuta" name="venRuta" required readonly/>
      <br />
        <label class="title" for="cobRuta">Cobrador</label>
        <input type="number" class="inpForm" value="{{cot.cobRuta}}" id="cobRuta" name="cobRuta" required readonly/>
      <br />
          <label style="background-color: rgb(255, 200, 98);" for="monto">Descuento De Saldo Anterior</label>
            <input style="border-color: rgb(255, 200, 98);" class="inpForm" type="number" value="{{cot.salAnt}}" id="SalAnt" name="salAnt" >
            <br>
      <label class="title" for="monto">Monto</label>
        <input type="number" class="inpForm" value="{{cot.monto}}" id="monto" name="monto" required readonly/>
      <br />
            <label class="title" for="monto">Adelanto</label>
        <input type="number" class="inpForm" value="{{cot.adelanto}}" id="adelanto" name="adelanto" readonly/>
      <br />
      <label class="title" for="codProd">Codigo De Producto</label>
      <select type="text" class="inpForm"  id="codProd" name="codProd" multiple >
        {{#each arrayCod}}
           <option value="{{cod}}" selected>{{cod}}</option>
        {{/each}}
      </select>
      <br />
      <label class="title" for="">Cantidad De Productos</label>
      {{#each arrayCod}}
        <br>
      <input type="number" class="inpForm" class="" style="width: 12%;" value="{{cant}}"  id="{{cod}}" name="{{cod}}" readonly/>
      <label class="title" for="">{{cod}} / {{nombre}}</label>
        {{/each}}
        <br>
      <label class="title" for="telefono">Descripcion</label>
      <input type="text" class="inpForm" value="{{cot.detalle}}" id="detalle" name="detalle" readonly/>
      <br />
      <label class="title" for="email">Total</label>
      <input type="number" class="inpForm" value="{{mTotal}}" id="mTotal" name="mTotal" readonly/>
      <br />
      <label class="title" for="descuento">Descuento Aplicado En %</label>
      <input type="number" class="inpForm" value="{{cot.descuento}}" id="descuento" name="descuento" readonly/>
      <br />
      <label class="title" for="categoria">Categoria</label>
       <input type="text" class="inpForm" value="{{planCot.categoria}}" id="categoria" name="categoria" readonly/>
      <br />
       <label class="title" for="plan">Plan</label>
       <input type="text" class="inpForm" value="{{planCot.plan}}" id="plan" name="plan" readonly/>
      <br />
       <label class="title" for="cuotas">N° de Cuotas</label>
       <input type="number" class="inpForm" value="{{planCot.cuotas}}" id="cuotas" name="cuotas" readonly/>
      <br />
        <label class="title" for="cuota">Valor Cuota</label>
       <input type="number" class="inpForm" value="{{cuota}}" id="cuota" name="cuota" readonly/>
      <br />
        <label class="title" for="diaDeCobro">Dia De Cobro</label>
       <input type="text" class="inpForm" value="{{cot.diaDeCobro}}" id="diaDeCobro" name="diaDeCobro" readonly/>
      <br />
      <label class="title" for="fecha">Fecha De Inicio</label>
    <input type="text" name="fecha" id="fecha" value="{{cot.fecha}}" class="inpForm" readonly>

      <div class="cantBtnForm">
    
        <a class="btnFormReset" href="javascript:history.back()"> Volver Atrás</a>

        <button class="btnFormAceptar" onclick="return confirm('¿Desea guardar la venta?')" type="submit">Guardar</button>
      </div>
  
    </form>
</div>
   
  </div>
